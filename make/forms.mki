# ---------------------------------------------------------------------------- #
# Libraries
# ---------------------------------------------------------------------------- #

FORMS_LIBS = \
	gskgen.plx \
	OFG4HPL.plx \
	$(ENDOFLIST)

# ---------------------------------------------------------------------------- #
# Reference forms
# ---------------------------------------------------------------------------- #

REF_FORMS = \
	gskref.fmb \
	$(ENDOFLIST)

# ---------------------------------------------------------------------------- #
# Icons
# ---------------------------------------------------------------------------- #

ICONS = \
	abort.ico \
	afclrall.ico \
	afclrrw.ico \
	afdelrw.ico \
	afedit.ico \
	affind.ico \
	afflddwn.ico \
	affldlft.ico \
	affldrt.ico \
	affldup.ico \
	afhelp.ico \
	afinsrw.ico \
	aflist.ico \
	afprint.ico \
	afquery.ico \
	afsave.ico \
	entqry.ico \
	keys.ico \
	$(ENDOFLIST)

# ---------------------------------------------------------------------------- #
# Forms. Case sensitive!
# ---------------------------------------------------------------------------- #

FORMS = \
	pm_about.fmx \
	pm_start.fmx \
	$(ENDOFLIST)

pm_start.fmb: pm_start-$(VER_DEV2000).fmt
	$(FMBGENCMD)

# $? might be report/pm_start-50.fmt hence use the OS PATHSEP to convert to 
# report\...

pm_start-60.fmt: pm_start-50.fmt
	$(CP) $(subst /,$(PATHSEP),$?) $@

# Remove pm_start-60.fmt

.INTERMEDIATE: pm_start-60.fmt

# ---------------------------------------------------------------------------- #
# Menus. Case sensitive!
# ---------------------------------------------------------------------------- #

MENUS = \
	pm_mnu.mmx \
	$(ENDOFLIST)

# ---------------------------------------------------------------------------- #
# Dependencies
# ---------------------------------------------------------------------------- #

gskgen.pll: OFG4HPL.pll

gskref.fmb: gskgen.pll

$(MENUS:.mmx=.mmb) $(FORMS:.fmx=.fmb): $(FORMS_LIBS:.plx=.pll)

$(FORMS:.fmx=.fmb): $(REF_FORMS)

# ---------------------------------------------------------------------------- #
# End of dependencies
# ---------------------------------------------------------------------------- #
