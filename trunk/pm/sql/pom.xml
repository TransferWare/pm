  <modelVersion>4.0.0</modelVersion>
  <groupId>com.transfer-solutions.performance-monitor</groupId>
  <artifactId>sql</artifactId>
  <packaging>pom</packaging>
  <description>The Performance Monitor utility database objects</description>

  <properties>
    <db.skip.generate.schema.full>false</db.skip.generate.schema.full>
    <!-- do not generate alter scripts -->
    <db.skip.generate.schema.incr>true</db.skip.generate.schema.incr>
    <db.schema>PM_OWNER</db.schema>
    <db.full.skip.install.sql>no</db.full.skip.install.sql> <!-- generate (un)install.sql -->
    <db.object.names>PM
,PM_DPI
,PM_MPI
,PM_RUN
,PM_RUN_SEQ
,PM_SESSION
,PM_SESSION_TMP
,PM_SQL
,PM_SQLAREA
,PM_SQLAREA_TMP
,PM_SQL_CURRENT
,PM_SQL_ID
,PM_SQL_SEQ
,PM_SYSSTAT
,PM_SYSSTAT_TMP
,PM_SYSTEM_EVENT
,PM_SYSTEM_EVENT_TMP
,PM_VW_SQLAREA
    </db.object.names>
    <db.object.names.include>1</db.object.names.include>
  </properties>
  
</project>

