\par \pard\plain \s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\rtlch\afs24\lang255\ltrch\dbch\af9\afs20\langfe1033\loch\f2\fs20\lang1043 {\ltrch\loch\f2 De volgende SQL*Plus opdracht initieert het verwijderen van informatie uit de databases aangeduid door database links MYDB1 en MYDB2 met behoud van 14 dagen historie. De job wordt elke dag om 00.30 uur gestart.}
\par \pard\plain \s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\rtlch\afs24\lang255\ltrch\dbch\af9\afs20\langfe1033\loch\f2\fs20\lang1043\ltrch\dbch\afs16\loch\f7\fs16 
\par {\ltrch\loch\f7 SQL> begin}
\par {\ltrch\loch\f7 SQL>\tab dbms_job.isubmit( }
\par {\ltrch\loch\f7 SQL>\tab \tab job => 101, }
\par {\ltrch\loch\f7 SQL>\tab \tab what => 'begin pm.cleanup(''MYDB1'', 14); pm.cleanup(''MYDB2'', 14); end;', }
\par {\ltrch\loch\f7 SQL>\tab \tab next_date => trunc(sysdate) + 1/(24*2), }
\par {\ltrch\loch\f7 SQL>\tab \tab interval  => 'trunc(sysdate) + 1 + 1/(24 * 2)' }
\par {\ltrch\loch\f7 SQL>\tab );}
\par {\ltrch\loch\f7 SQL> end;}
\par {\ltrch\loch\f7 SQL> /}
\par {\ltrch\loch\f7 SQL> commit;}
