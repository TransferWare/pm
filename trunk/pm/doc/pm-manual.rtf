\par }{\hich\af0\dbch\af0\loch\f1 Het volgende script compileert alle Forms \hich\af0\dbch\af0\loch\f1 en Reports. De volgende environment variabele moet gezet zijn: userid. Het script moet gestart worden vanuit de install directory.
\par 
\par }{\f2\fs16 #!/bin/sh -exu
\par frmgen=f90genm.sh
\par rptgen=rwconverter.sh
\par bindir=../bin
\par }{\f2\fs16\lang1033 srddir=../reports
\par 
\par # WINDOWS_STATE=MINIMIZE does not work for Dutch
\par NLS_LANG=AMERICAN_AMERICA.WE8ISO8859P1
\par }{\f2\fs16 export NLS_LANG
\par 
\par test -d $bindir || mkdir $bindir
\par 
\par }{\f2\fs16\lang1033 for pld in $srcdir/*.pld
\par do
\par   pll=`basename $pld .pld`.pll
\par   plx=`basename $pld .pld`.plx
\par }\pard\plain \s104\nowidctlpar\adjustright \f2\fs16\cgrid {  $frmgen USERID=$userid MODULE=$pld MODULE_TYPE=LIBRARY OUTPUT_FILE=$pll BATCH=YES PARSE=YES \\
\par WINDOW_STATE=MINIMIZE
\par }\pard\plain \nowidctlpar\adjustright \fs20\lang1043\cgrid {\f2\fs16\lang1033   $frmgen USERID=$userid MODULE=$pll MODULE_TYPE=LIBRARY OUTPUT_FILE=$plx BATCH=YES \\
\par WINDOW_STATE=MINIMIZE
\par 
\par   mv $plx $bindir
\par done
\par 
\par for fmt in $srcdir/*.fmt
\par do
\par   fmb=`basename $fmt .fmt`.fmb
\par   fmx=`basename $fmt .fmt`.fmx
\par   $frmgen USERID=$userid MODULE=$fmt MODULE_TYPE=FORM OUTPUT_FILE=$fmb BATCH=YES PARSE=YES \\
\par WINDOW_STATE=MINIMIZE
\par 
\par   # gskgen.fmb is a reference form
\par   if [ "$fmb" = "gskgen.fmb" ]
\par   then
\par     continue
\par   fi
\par 
\par   $frmgen USERID=$userid MODULE=$fmb MODULE_TYPE=FORM OUTPUT_FILE=$fmx BATCH=YES \\
\par WINDOW_STATE=MINIMIZE
\par 
\par   mv $fmx $bindir
\par done
\par 
\par for mmt in $srcdir/*.mmt
\par do
\par   mmb=`basename $mmt .mmt`.mmb
\par   }{\f2\fs16\lang1036 mmx=`basename $mmt .mmt`.mmx
\par   }{\f2\fs16\lang1033 $frmgen USERID=$userid MODULE=$mmt MODULE_TYPE=MENU OUTPUT_FILE=$mmb BATCH=YES PARSE=YES \\
\par WINDOW_STATE=MINIMIZE
\par   $frmgen USERID=$userid MODULE=$mmb MODULE_TYPE=MENU OUTPUT_FILE=$mmx BATCH=YES \\
\par WINDOW_STATE=MINIMIZE
\par 
\par   mv $mmx $bindir
\par done
\par 
\par for rex in $srcdir/*.rex
\par do
\par   rdf=`basename $rex .rex`.rdf
\par   rep=`basename $rex .rex`.rep
\par   $rptgen USERID=$userid SOURCE=$rex STYPE=rexfile DEST=$rdf DTYPE=rdffile BATCH=YES OVERWRITE=YES
\par   $rptgen USERID=$userid SOURCE=$rdf STYPE=rdffile DEST=$rep DTYPE=repfile BATCH=YES OVERWRITE=YES
\par 
\par   mv $rep $bindir
\par done
\par 
