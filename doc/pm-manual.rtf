rm\par   if [ "$fmb" = "gskgen.fmb" ]\par   then\par     continue\par   fi\par \par   $frmgen USERID=$userid MODULE=$fmb MODULE_TYPE=FORM OUTPUT_FILE=$fmx BATCH=YES \\\par WINDOW_STATE=MINIMIZE\par \par   mv $fmx $bindir\par done\par \par for mmt in $srcdir/*.mmt\par do\par   mmb=`basename $mmt .mmt`.mmb\par   mmx=`basename $mmt .mmt`.mmx\par   $frmgen USERID=$userid MODULE=$mmt MODULE_TYPE=MENU OUTPUT_FILE=$mmb BATCH=YES PARSE=YES \\\par WINDOW_STATE=MINIMIZE\par   $frmgen USERID=$userid MODULE=$mmb MODULE_TYPE=MENU OUTPUT_FILE=$mmx BATCH=YES \\\par WINDOW_STATE=MINIMIZE\par \par   mv $mmx $bindir\par done\par \par for rex in $srcdir/*.rex\par do\par   rdf=`basename $rex .rex`.rdf\par   rep=`basename $rex .rex`.rep\par   $rptgen USERID=$userid SOURCE=$rex STYPE=rexfile DEST=$rdf DTYPE=rdffile BATCH=YES OVERWRITE=YES\par   $rptgen USERID=$userid SOURCE=$rdf STYPE=rdffile DEST=$rep DTYPE=repfile BATCH=YES OVERWRITE=YES\par \par   mv $rep $bindir\par done\par \par cp $srcdir/*.gif $bindir\par rm *.pll *.mmb *.fmb *.rdf\par }}